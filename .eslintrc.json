{
  "extends": "airbnb-base",
  "env": {
     "es6": true,
     "browser": true,
     "jest": true
  },
  "rules": {
    "camelcase": "off",
    "class-methods-use-this": 0,
    "no-unused-expressions": 0,
    "no-unused-vars": 0,
    "no-param-reassign": 0,
    "comma-dangle": ["error", "never"],
    "no-restricted-syntax": [
      "error",
      "LabeledStatement",
      "WithStatement"
    ]
  }
}